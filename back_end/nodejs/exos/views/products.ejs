<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title></title>
</head>
<body>

	<h1>Products</h1>
	<table>
	<% 	
		for(var i=0; i < products.length; i++) 
		{
	%>
			<tr>
				<td><%= products[i].name %></td>
				<td><%= products[i].price %></td>
				<td><a target="_blank" href="/products/<%= products[i].reference %>">voir</a></td>
			</tr>
	<% 	} %>
	</table>
	
</body>
</html>